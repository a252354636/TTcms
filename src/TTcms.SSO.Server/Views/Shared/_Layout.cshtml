    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/admin-lte/css")
    @Styles.Render("~/bootstrap-table/css")
    @Styles.Render("~/bootstrapValidator/css")
    @Styles.Render("~/fileinput/css")
    @Styles.Render("~/toastr/css")
    @Styles.Render("~/select2/css")
    @Styles.Render("~/Content/css")
